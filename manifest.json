{
  "description": "Normalize and redisplay stars that are used for evaluation in restaurant reviews evaluation site「Tabelog」",
  "manifest_version": 2,
  "name": "TabelogStarNormalize",
  "version": "1.0.1",
  "homepage_url": "https://github.com/takedajs/TabelogStarNormalize",
  "icons": {
    "48": "icons/60.png"
  },
  "content_scripts": [{
      "matches": [
        "http://tabelog.com/*",
        "https://tabelog.com/*",
        "http://s.tabelog.com/*",
        "https://s.tabelog.com/*"
      ],
      "js": ["content-script.js"],
      "run_at": "document_end"
  }]
}
